all: adlmidi



adlmidi: tmp/midiplay.o tmp/puzzlegame.o tmp/input.o libADLMIDI.so
	$(CXXLINK)  $^  $(DEBUG) $(SDL) -o $@ $(LDLIBS) -L. -lADLMIDI

tmp/midiplay.o: utils/adlmidi-2/midiplay.cc tmp
	$(CXX) $(CPPFLAGS) $<  $(DEBUG)  -c -o $@

tmp/puzzlegame.o: utils/adlmidi-2/puzzlegame.cc tmp
	$(CXX) $(CPPFLAGS) $<  $(DEBUG)  -c -o $@

tmp/input.o: utils/adlmidi-2/input.cc tmp
	$(CXX) $(CPPFLAGS) $<  $(DEBUG)  -c -o $@



libADLMIDI.so: tmp/adldata.o tmp/adlmidi.o \
    tmp/adlmidi_load.o tmp/adlmidi_midiplay.o tmp/adlmidi_midiplay.o \
    tmp/adlmidi_opl3.o tmp/adlmidi_private.o tmp/adlmidi_sequencer.o \
    tmp/dosbox_opl3.o tmp/nuked_opl3.o tmp/nuked_opl3_v174.o \
    tmp/dbopl.o tmp/nukedopl3.o tmp/nukedopl3_174.o tmp/wopl_file.o
	$(CXXLINKLIB) $^  $(DEBUG) -o $@ $(LDLIBS)

tmp/adldata.o: src/adldata.cpp tmp
	$(CXX) $(CPPFLAGS_LIB) $<  $(DEBUG)  -c -o $@

tmp/adlmidi.o: src/adlmidi.cpp tmp
	$(CXX) $(CPPFLAGS_LIB) $<  $(DEBUG)  -c -o $@

tmp/adlmidi_load.o: src/adlmidi_load.cpp tmp
	$(CXX) $(CPPFLAGS_LIB) $<  $(DEBUG)  -c -o $@

tmp/adlmidi_midiplay.o: src/adlmidi_midiplay.cpp tmp
	$(CXX) $(CPPFLAGS_LIB) $<  $(DEBUG)  -c -o $@

tmp/adlmidi_opl3.o: src/adlmidi_opl3.cpp tmp
	$(CXX) $(CPPFLAGS_LIB) $<  $(DEBUG)  -c -o $@

tmp/adlmidi_private.o: src/adlmidi_private.cpp tmp
	$(CXX) $(CPPFLAGS_LIB) $<  $(DEBUG)  -c -o $@

tmp/adlmidi_sequencer.o: src/adlmidi_sequencer.cpp tmp
	$(CXX) $(CPPFLAGS_LIB) $<  $(DEBUG)  -c -o $@

tmp/dosbox_opl3.o: src/chips/dosbox_opl3.cpp tmp
	$(CXX) $(CPPFLAGS_LIB) $<  $(DEBUG)  -c -o $@

tmp/nuked_opl3.o: src/chips/nuked_opl3.cpp tmp
	$(CXX) $(CPPFLAGS_LIB) $<  $(DEBUG)  -c -o $@

tmp/nuked_opl3_v174.o: src/chips/nuked_opl3_v174.cpp tmp
	$(CXX) $(CPPFLAGS_LIB) $<  $(DEBUG)  -c -o $@

tmp/dbopl.o: src/chips/dosbox/dbopl.cpp tmp
